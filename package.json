    "devDependencies": {
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^24.0.17",
        "@types/jsonwebtoken": "^8.3.3",
        "@types/node": "^12.7.1",
        "@types/node-fetch": "^2.5.0",
        "@types/request": "^2.48.2",
        "inquirer": "^6.5.1",
        "jest": "^24.8.0",
        "jest-extended": "^0.11.2",
        "jsonwebtoken": "^8.5.1",
        "request": "^2.88.0",
        "shelljs": "^0.8.3",
        "ts-jest": "^24.0.2",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "typestrict": "^1.0.2"
    },
    "prettier": {
        "printWidth": 160,
        "tabWidth": 4,
        "trailingComma": "es5",
        "bracketSpacing": false,
        "jsxBracketSameLine": true,
        "arrowParens": "always"
    }
