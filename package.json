    "devDependencies": {
        "@types/inquirer": "^0.0.43",
        "@types/jest": "^23.3.5",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/node": "^10.12.0",
        "@types/node-fetch": "^2.1.2",
        "@types/request": "^2.47.1",
        "babel-core": "^6.26.3",
        "inquirer": "^6.2.0",
        "jest": "^23.6.0",
        "jest-extended": "^0.11.0",
        "jsonwebtoken": "^8.3.0",
        "request": "^2.88.0",
        "shelljs": "^0.8.2",
        "ts-jest": "^23.10.4",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.1.3",
        "typestrict": "^1.0.1"
    },
    "prettier": {
        "printWidth": 160,
        "tabWidth": 4,
        "trailingComma": "es5",
        "bracketSpacing": false,
        "jsxBracketSameLine": true,
        "arrowParens": "always"
    }
